{
  "name": "acalvino4/twig-nullsafe",
  "description": "Adds a nullsafe operator '?.' to twig.",
  "version": "0.1.0",
  "homepage": "https://github.com/acalvino4/twig-nullsafe",
  "license": "mit",
  "authors": [
    {
      "name": "Augustine Calvino",
      "homepage": "https://github.com/acalvino4"
    }
  ],
  "require": {
    "php": ">=8.1",
    "twig/twig": "^3.3"
  },
  "require-dev": {
    "craftcms/phpstan": "dev-main",
    "craftcms/ecs": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "acalvino4\\nullsafe\\": "src/"
    }
  },
  "scripts": {
    "test": "ecs check --ansi && phpstan --memory-limit=1G",
    "fix": "ecs check --ansi --fix"
  },
  "config": {
    "sort-packages": true,
    "platform": {
      "php": "8.2"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
